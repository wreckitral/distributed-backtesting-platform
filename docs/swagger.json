{
    "schemes": [
        "http",
        "https"
    ],
    "swagger": "2.0",
    "info": {
        "description": "API for distributed financial backtesting platform",
        "title": "Distributed Backtesting Platform API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.swagger.io/support",
            "email": "support@swagger.io"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/api/v1/backtests": {
            "get": {
                "description": "Get a list of all backtests",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "backtests"
                ],
                "summary": "List all backtests",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.ListResponse"
                        }
                    }
                }
            },
            "post": {
                "description": "Create and execute a backtest with the given parameters",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "backtests"
                ],
                "summary": "Create a new backtest",
                "parameters": [
                    {
                        "description": "Backtest parameters",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.CreateBacktestRequest"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created",
                        "schema": {
                            "$ref": "#/definitions/dto.BacktestResponse"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/api/v1/backtests/{id}": {
            "get": {
                "description": "Get details of a specific backtest",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "backtests"
                ],
                "summary": "Get backtest by ID",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Backtest ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.BacktestResponse"
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    }
                }
            },
            "delete": {
                "description": "Delete a backtest and all associated data",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "backtests"
                ],
                "summary": "Delete a backtest",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Backtest ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.SuccessResponse"
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/api/v1/backtests/{id}/metrics": {
            "get": {
                "description": "Get performance metrics for a specific backtest",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "backtests"
                ],
                "summary": "Get backtest metrics",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Backtest ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.MetricsResponse"
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/api/v1/backtests/{id}/trades": {
            "get": {
                "description": "Get all trades for a specific backtest",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "backtests"
                ],
                "summary": "Get backtest trades",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Backtest ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.ListResponse"
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/health": {
            "get": {
                "description": "Check if the API is running",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "health"
                ],
                "summary": "Health check",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "dto.BacktestResponse": {
            "type": "object",
            "properties": {
                "created_at": {
                    "type": "string",
                    "example": "2025-01-15T10:30:00Z"
                },
                "end_date": {
                    "type": "string",
                    "example": "2024-12-31"
                },
                "id": {
                    "type": "string",
                    "example": "123e4567-e89b-12d3-a456-426614174000"
                },
                "initial_capital": {
                    "type": "number",
                    "example": 10000
                },
                "start_date": {
                    "type": "string",
                    "example": "2024-01-01"
                },
                "status": {
                    "type": "string",
                    "example": "completed"
                },
                "strategy_id": {
                    "type": "string",
                    "example": "SMA Crossover"
                },
                "symbol": {
                    "type": "string",
                    "example": "AAPL"
                },
                "updated_at": {
                    "type": "string",
                    "example": "2025-01-15T10:35:00Z"
                }
            }
        },
        "dto.CreateBacktestRequest": {
            "type": "object",
            "required": [
                "end_date",
                "initial_capital",
                "start_date",
                "strategy_id",
                "symbol"
            ],
            "properties": {
                "end_date": {
                    "type": "string",
                    "example": "2024-12-31"
                },
                "initial_capital": {
                    "type": "number",
                    "example": 10000
                },
                "start_date": {
                    "type": "string",
                    "example": "2024-01-01"
                },
                "strategy_id": {
                    "type": "string",
                    "example": "buy_hold"
                },
                "symbol": {
                    "type": "string",
                    "example": "AAPL"
                }
            }
        },
        "dto.ErrorResponse": {
            "type": "object",
            "properties": {
                "error": {
                    "type": "string",
                    "example": "Invalid request"
                },
                "message": {
                    "type": "string",
                    "example": "strategy field is required"
                }
            }
        },
        "dto.ListResponse": {
            "type": "object",
            "properties": {
                "items": {},
                "limit": {
                    "type": "integer",
                    "example": 20
                },
                "page": {
                    "type": "integer",
                    "example": 1
                },
                "total": {
                    "type": "integer",
                    "example": 42
                }
            }
        },
        "dto.MetricsResponse": {
            "type": "object",
            "properties": {
                "backtest_id": {
                    "type": "string",
                    "example": "123e4567-e89b-12d3-a456-426614174000"
                },
                "losing_trades": {
                    "type": "integer",
                    "example": 2
                },
                "max_drawdown": {
                    "type": "number",
                    "example": 15.5
                },
                "profit_factor": {
                    "type": "number",
                    "example": 4.33
                },
                "return_pct": {
                    "type": "number",
                    "example": 25
                },
                "sharpe_ratio": {
                    "type": "number",
                    "example": 1.82
                },
                "total_return": {
                    "type": "number",
                    "example": 2500
                },
                "total_trades": {
                    "type": "integer",
                    "example": 8
                },
                "win_rate": {
                    "type": "number",
                    "example": 75
                },
                "winning_trades": {
                    "type": "integer",
                    "example": 6
                }
            }
        },
        "dto.SuccessResponse": {
            "type": "object",
            "properties": {
                "data": {},
                "message": {
                    "type": "string",
                    "example": "Backtest created successfully"
                }
            }
        }
    }
}